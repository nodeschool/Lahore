@font-face {
  font-family: 'ATC Harris';
  src: url('../fonts/atcharris-regular-webfont.eot');
  src: url('../fonts/atcharris-regular-webfont.eot?#iefix') format('embedded-opentype'),
    url('../fonts/atcharris-regular-webfont.woff2') format('woff2'),
    url('../fonts/atcharris-regular-webfont.woff') format('woff'),
    url('../fonts/atcharris-regular-webfont.ttf') format('truetype'),
    url('../fonts/atcharris-regular-webfont.svg#atc_harrisregular') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'ATC Harris';
  src: url('../fonts/atcharris-bold-webfont.eot');
  src: url('../fonts/atcharris-bold-webfont.eot?#iefix') format('embedded-opentype'),
    url('../fonts/atcharris-bold-webfont.woff2') format('woff2'),
    url('../fonts/atcharris-bold-webfont.woff') format('woff'),
    url('../fonts/atcharris-bold-webfont.ttf') format('truetype'),
    url('../fonts/atcharris-bold-webfont.svg#atc_harrisbold') format('svg');
  font-weight: bold;
  font-style: normal;
}
